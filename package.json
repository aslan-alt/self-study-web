{
  "name": "nextjs-blog-1",
  "version": "0.1.0",
  "private": true,
  "git-hooks": {
    "pre-push": "next lint --ext .ts,.tsx --fix && yarn typeCheck && yarn prettier"
  },
  "scripts": {
    "lint": "next lint --fix",
    "typeCheck": "tsc --noEmit -p tsconfig.json",
    "dev": "next dev",
    "prettier": "prettier --list-different --write '{pages,src,cypress,lib,__tests__}/**/*.{ts,tsx,js,jsx}' ",
    "build": "next build",
    "test": "jest",
    "e2e:dev": "cypress open",
    "e2e:run": "cypress run",
    "start": "next build && next start",
    "ci": "start-server-and-test start http://localhost:3000 e2e:run",
    "reboot": "rm -rf node_modules && yarn",
    "typeorm": "typeorm -d dist/ormconfig.js",
    "typeorm:build": "babel ./db --out-dir dist --extensions .ts,.tsx",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create",
    "seed": "rm -rf dist && yarn typeorm:build && node dist/seed.js",
    "update:package": "ncu -u"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@babel/cli": "^7.19.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/md5": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "antd": "^5.0.1",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "iron-session": "^6.3.1",
    "lodash": "^4.17.21",
    "marked": "^4.2.3",
    "md5": "^2.3.0",
    "next": "13.0.5",
    "next-iron-session": "^4.1.14",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.56.1",
    "styled-components": "^5.3.6",
    "typeorm": "^0.3.10",
    "ua-parser-js": "^1.0.32"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.3.1",
    "@types/formidable": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.190",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/ua-parser-js": "^0.7.36",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^11.2.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next-images": "^1.8.4",
    "prettier": "^2.8.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
